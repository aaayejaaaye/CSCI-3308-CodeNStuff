DROP TABLE user_info;
CREATE TABLE IF NOT EXISTS user_info(
  user_name VARCHAR(30),  /* Name of the user */
  feedback_ratings SMALLINT NOT NULL, /* one of rating feedbacks from the user 0 means worst, 100 means best*/
  user_occupation VARCHAR(30), /* user's occupation: student, data scientist..etc */
  birth_date DATE NOT NULL,   /* user's birthday, can be used with name as a primary key */
  users INT[] NOT NULL,       /* This is an array which has the user ids. can be used as a foreign key to users */
  PRIMARY KEY (user_name, birth_date) /* unique primary key for each user*/
);

/* this one is more personal, for user's record. */

DROP TABLE user_profile;
CREATE TABLE IF NOT EXISTS user_profile(
  id SERIAL PRIMARY KEY, /* Unique id for each user */
  password VARCHAR(50) NOT NULL, /* user's password */
  searching_history VARCHAR(200), /* website link of users frequently used */
  personal_description VARCHAR(200), /* user's personal description */
  img_src VARCHAR(200) /* user's profile image */
);

